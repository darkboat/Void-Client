package me.rexysaur.void_.Client.cosmetics;

import net.minecraft.util.ResourceLocation;

public class CosmeticCape {
	public String name;
	public int price;
	public ResourceLocation resource;
	
	public CosmeticCape(String name, int price, String path)
	{
		this.name = name;
		this.price = price;
		
		this.resource = new ResourceLocation(path);
	}
}
