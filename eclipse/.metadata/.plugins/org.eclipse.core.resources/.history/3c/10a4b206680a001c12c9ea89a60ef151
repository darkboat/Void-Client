package me.rexysaur.void_.Client.util;

public enum Colours {
	WHITE(255, 255, 255),
	BLACK(0, 0, 0);
	
	public int red;
	public int green;
	public int blue;
	
	Colours(int... RGB)
	{
		this.red = RGB[0];
		this.green = RGB[1];
		this.blue = RGB[2];
	}
	
	Colours getColour(Colours col)
	{
		return col;
		
	}
}
