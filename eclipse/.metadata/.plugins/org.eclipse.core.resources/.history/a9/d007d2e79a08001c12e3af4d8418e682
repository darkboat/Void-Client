package me.rexysaur.void_.Client.hud.mod.impl;

import me.rexysaur.void_.Client.hud.mod.HudMod;
import net.minecraft.client.gui.Gui;

public class FPSMod extends HudMod {
	public FPSMod()
	{
		super("FPS Counter", 5, 5);
	}
	
	@Override
	public void draw()
	{
		Gui.drawRect(getX() - 2,  getY() - 2, getX() + getWidth(), getY() + getHeight(), new Color(0, 0, 0, 170));
	}
}
