package me.rexysaur.void_.Client.util;

import java.awt.Color;

public enum Colours {
	WHITE(255, 255, 255),
	BLACK(0, 0, 0);
	
	public int red;
	public int green;
	public int blue;
	
	Colours(int... RGB)
	{
		this.red = RGB[0];
		this.green = RGB[1];
		this.blue = RGB[2];
	}
	
	Colours getColour()
	{
		return new Color(this.red, this.green, this.blue);
	}
}
