package me.rexysaur.void_.Client.cosmetics;

import java.awt.Color;

public class CosmeticHat {
	private String name;
	private int price;
	private Color col;
	
	public CosmeticHat(String name, int price, Color col)
	{
		
	}
	
	public String getname()
	{
		return this.name;
	}
	
	public int getPrice()
	{
		return this.price;
	}
	
	public Color getColor()
	{
		return this.col;
	}
}
